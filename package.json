{
  "dependencies": {
    "@module-federation/nextjs-mf": "8.8.56",
    "@tailwindcss/postcss": "4.2.1",
    "clsx": "2.1.1",
    "next": "14.2.35",
    "postcss": "8.5.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tailwind-merge": "3.5.0",
    "tailwind-variants": "3.2.2",
    "tailwindcss": "4.2.1"
  },
  "description": "Root workspace for the easy-web micro-frontend monorepo.",
  "devDependencies": {
    "@commitlint/cli": "20.4.2",
    "@commitlint/config-conventional": "20.4.2",
    "@commitlint/types": "20.4.0",
    "@eslint/compat": "2.0.2",
    "@eslint/eslintrc": "3.3.4",
    "@eslint/js": "10.0.1",
    "@next/bundle-analyzer": "14.2.35",
    "@next/eslint-plugin-next": "14.2.35",
    "@nx/eslint": "22.5.2",
    "@nx/eslint-plugin": "22.5.2",
    "@nx/js": "22.5.2",
    "@nx/next": "22.5.2",
    "@nx/vite": "22.5.2",
    "@nx/vitest": "22.5.2",
    "@nx/workspace": "22.5.2",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.2",
    "@types/node": "25.3.0",
    "@types/react": "18.3.28",
    "@types/react-dom": "18.3.7",
    "@vitejs/plugin-react": "5.1.4",
    "@vitest/coverage-v8": "4.0.18",
    "eslint": "9.39.3",
    "eslint-config-next": "14.2.35",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "husky": "9.1.7",
    "jsdom": "28.1.0",
    "lint-staged": "16.2.7",
    "nx": "22.5.2",
    "postcss-load-config": "6.0.1",
    "prettier": "3.8.1",
    "prettier-plugin-tailwindcss": "0.7.2",
    "stylelint": "17.4.0",
    "stylelint-config-standard": "40.0.0",
    "stylelint-order": "7.0.1",
    "tslib": "2.8.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.56.1",
    "vite": "7.3.1",
    "vitest": "4.0.18",
    "webpack": "5.105.3"
  },
  "engines": {
    "node": ">=20"
  },
  "keywords": [
    "easy-web",
    "frontend",
    "module-federation",
    "monorepo",
    "next",
    "nx",
    "react",
    "typescript"
  ],
  "license": "MIT",
  "name": "@easy-web/workspace",
  "packageManager": "pnpm@10.30.3",
  "pnpm": {
    "peerDependencyRules": {
      "allowAny": [
        "eslint"
      ]
    },
    "overrides": {
      "eslint": "9.39.3"
    }
  },
  "private": true,
  "scripts": {
    "ci": "pnpm reset && pnpm format:check && pnpm lint:workspace && pnpm nx affected -t lint stylelint typecheck test build",
    "ci:all": "pnpm reset && pnpm format:check && pnpm lint:workspace && pnpm nx run-many -t lint stylelint typecheck test build",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "lint": "nx affected -t lint",
    "lint:fix": "nx affected -t lint --fix",
    "lint:all": "nx run-many -t lint",
    "lint:all:fix": "nx run-many -t lint --fix",
    "stylelint": "nx affected -t stylelint",
    "stylelint:fix": "nx affected -t stylelint --fix",
    "stylelint:all": "nx run-many -t stylelint",
    "stylelint:all:fix": "nx run-many -t stylelint --fix",
    "lint:workspace": "eslint \"*.{js,jsx,mjs,mts,ts,tsx}\" --cache --cache-location .cache/eslint/.eslintcache",
    "lint:workspace:fix": "eslint \"*.{js,jsx,mjs,mts,ts,tsx}\" --fix --cache --cache-location .cache/eslint/.eslintcache",
    "preinstall": "npx -y only-allow pnpm",
    "prepare": "husky",
    "reset": "nx reset",
    "test": "nx affected -t test",
    "test:all": "nx run-many -t test",
    "test:coverage": "nx affected -t test --coverage",
    "test:all:coverage": "nx run-many -t test --coverage",
    "test:watch": "nx affected -t test --watch",
    "test:all:watch": "nx run-many -t test --watch",
    "typecheck": "nx affected -t typecheck",
    "typecheck:all": "nx run-many -t typecheck"
  },
  "version": "1.0.0"
}
