{
  "dependencies": {
    "@module-federation/nextjs-mf": "8.8.56",
    "@tailwindcss/postcss": "4.2.1",
    "clsx": "2.1.1",
    "next": "14.2.35",
    "postcss": "8.5.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tailwind-merge": "3.5.0",
    "tailwind-variants": "3.2.2",
    "tailwindcss": "4.2.1"
  },
  "description": "Root workspace for the easy-web micro-frontend monorepo.",
  "devDependencies": {
    "@commitlint/cli": "20.4.2",
    "@commitlint/config-conventional": "20.4.2",
    "@commitlint/types": "20.4.0",
    "@eslint/compat": "2.0.2",
    "@eslint/eslintrc": "3.3.4",
    "@eslint/js": "10.0.1",
    "@next/bundle-analyzer": "14.2.35",
    "@next/eslint-plugin-next": "14.2.35",
    "@nx/eslint": "22.5.2",
    "@nx/eslint-plugin": "22.5.2",
    "@nx/js": "22.5.2",
    "@nx/next": "22.5.2",
    "@nx/workspace": "22.5.2",
    "@swc-node/register": "1.11.1",
    "@swc/cli": "0.8.0",
    "@swc/core": "1.15.13",
    "@swc/helpers": "0.5.19",
    "@types/node": "25.3.0",
    "@types/react": "18.3.28",
    "@types/react-dom": "18.3.7",
    "eslint": "9.39.3",
    "eslint-config-next": "14.2.35",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "nx": "22.5.2",
    "prettier": "3.8.1",
    "prettier-plugin-tailwindcss": "0.7.2",
    "stylelint": "17.3.0",
    "stylelint-config-standard": "40.0.0",
    "stylelint-order": "7.0.1",
    "tslib": "2.8.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.56.1",
    "webpack": "5.105.2"
  },
  "engines": {
    "node": ">=20"
  },
  "keywords": [
    "easy-web",
    "frontend",
    "module-federation",
    "monorepo",
    "next",
    "nx",
    "react",
    "typescript"
  ],
  "license": "MIT",
  "name": "@easy-web/workspace",
  "packageManager": "pnpm@10.30.1",
  "pnpm": {
    "peerDependencyRules": {
      "allowAny": [
        "eslint"
      ]
    },
    "overrides": {
      "eslint": "9.39.3"
    }
  },
  "private": true,
  "scripts": {
    "ci": "pnpm nx format:check && pnpm eslint \"*.{cjs,cts,js,jsx,mjs,mts,ts,tsx}\" --cache --cache-location .cache/eslint/.eslintcache && pnpm nx affected -t lint lint:styles typecheck build",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "lint": "nx run-many -t lint",
    "lint:affected": "nx affected -t lint",
    "lint:fix": "nx run-many -t lint --fix",
    "lint:styles": "nx run-many -t lint:styles",
    "lint:styles:affected": "nx affected -t lint:styles",
    "lint:styles:fix": "nx run-many -t lint:styles --fix",
    "lint:workspace": "eslint \"*.{cjs,cts,js,jsx,mjs,mts,ts,tsx}\" --cache --cache-location .cache/eslint/.eslintcache",
    "lint:workspace:fix": "eslint \"*.{cjs,cts,js,jsx,mjs,mts,ts,tsx}\" --fix --cache --cache-location .cache/eslint/.eslintcache",
    "preinstall": "npx -y only-allow pnpm",
    "prepare": "husky",
    "reset": "nx reset",
    "typecheck": "nx run-many -t typecheck",
    "typecheck:affected": "nx affected -t typecheck"
  },
  "version": "1.0.0"
}
